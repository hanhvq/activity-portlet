#{param name=activity/}

<% 
   activityId = activity.id;
%>
<div class="activity-container">
    <div class="activity-data" data-activityid="${activityId}">
      <div class="title"><%=activity.title%></div>
    </div>
    <div class="row">
        <span class="like-container"></span>
    </div>

    <div class="row">
        <span class="comment-container"></span>
    </div>
</div>